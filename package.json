{
  "name": "aurelia-materialize",
  "version": "0.2.9",
  "description": "Aurelia integration with Materialize CSS Framework.",
  "keywords": [
    "aurelia",
    "materialize",
    "css",
    "plugin"
  ],
  "homepage": "https://github.com/manuel-guilbault/aurelia-materialize",
  "bugs": {
    "url": "https://github.com/manuel-guilbault/aurelia-materialize/issues"
  },
  "license": "MIT",
  "author": "Manuel Guilbault <manuel.guilbault@gmail.com> (https://github.com/manuel-guilbault/)",
  "main": "dist/commonjs/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/manuel-guilbault/aurelia-materialize"
  },
  "jspm": {
    "main": "index",
    "format": "amd",
    "directories": {
      "lib": "dist/amd"
    },
    "dependencies": {
      "aurelia-binding": "github:aurelia/binding@^0.7.3",
      "aurelia-dependency-injection": "github:aurelia/dependency-injection@^0.8.1",
      "aurelia-event-aggregator": "github:aurelia/event-aggregator@^0.5.0",
      "aurelia-templating": "github:aurelia/templating@^0.12.1",
      "css": "github:systemjs/plugin-css@^0.1.9",
      "jquery": "github:components/jquery@^2.1.4",
      "materialize": "github:Dogfalo/materialize@^0.97.0"
    },
    "devDependencies": {
      "traceur": "github:jmcriffey/bower-traceur@0.0.88",
      "traceur-runtime": "github:jmcriffey/bower-traceur-runtime@0.0.88"
    },
    "overrides": {
      "github:Dogfalo/materialize@0.97.0": {
        "main": "dist/js/materialize",
        "shim": {
          "dist/js/materialize": {
            "deps": [
              "jquery"
            ],
            "exports": "$"
          }
        },
        "dependencies": {
          "jquery": "github:components/jquery"
        }
      }
    }
  },
  "devDependencies": {
    "aurelia-tools": "^0.1.3",
    "conventional-changelog": "0.0.11",
    "del": "^1.1.0",
    "gulp": "^3.8.10",
    "gulp-babel": "^5.1.0",
    "gulp-bump": "^0.1.11",
    "gulp-jshint": "^1.9.0",
    "gulp-yuidoc": "^0.1.2",
    "jasmine-core": "^2.1.3",
    "jshint-stylish": "^1.0.0",
    "karma": "^0.12.28",
    "karma-babel-preprocessor": "^5.2.1",
    "karma-chrome-launcher": "^0.1.7",
    "karma-jasmine": "^0.3.5",
    "karma-jspm": "^1.1.5",
    "object.assign": "^1.0.3",
    "require-dir": "^0.1.0",
    "run-sequence": "^1.0.2",
    "vinyl-paths": "^1.0.0",
    "yargs": "^2.1.1"
  },
  "aurelia": {
    "usedBy": [],
    "documentation": {
      "links": [
        {
          "rel": "license",
          "mediaType": "text/plain",
          "title": "The MIT License (MIT)",
          "href": "LICENSE"
        },
        {
          "rel": "describedby",
          "mediaType": "application/aurelia-doc+json",
          "title": "API",
          "href": "doc/api.json"
        },
        {
          "rel": "version-history",
          "mediaType": "text/markdown",
          "title": "Change Log",
          "href": "doc/CHANGELOG.md"
        }
      ]
    }
  }
}
